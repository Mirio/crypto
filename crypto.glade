                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <widget class="GtkButton" id="conv_sha">
                <property name="label" translatable="yes">Testo-&gt;SHA512</property>
